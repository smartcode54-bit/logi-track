{
    "mcpServers": {
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        },
        "dart-mcp-server": {
            "command": "dart",
            "args": [
                "mcp-server"
            ],
            "env": {}
        },
        "firebase-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "@firebase/mcp@latest"
            ]
        },
        "next-devtools": {
            "command": "npx",
            "args": [
                "-y",
                "next-devtools@latest"
            ]
        }
    }
}